ine":56,"column":6},"source":"app.js","original":{"line":76,"column":6}},{"name":"addEventListener","generated":{"line":56,"column":7},"source":"app.js","original":{"line":76,"column":7}},{"generated":{"line":56,"column":23},"source":"app.js","original":{"line":76,"column":0}},{"generated":{"line":56,"column":24},"source":"app.js","original":{"line":76,"column":24}},{"generated":{"line":56,"column":36},"source":"app.js","original":{"line":76,"column":0}},{"name":"router","generated":{"line":56,"column":38},"source":"app.js","original":{"line":76,"column":38}},{"generated":{"line":56,"column":44},"source":"app.js","original":{"line":76,"column":0}},{"name":"router","generated":{"line":57,"column":0},"source":"app.js","original":{"line":78,"column":0}},{"generated":{"line":57,"column":6},"source":"app.js","original":{"line":78,"column":6}}],"sources":{"app.js":"const container = document.getElementById('root');\r\nconst ajax = new XMLHttpRequest();\r\nconst content = document.createElement('div');\r\nconst NEWS_URL = 'https://api.hnpwa.com/v0/news/1.json';\r\nconst CONTENT_URL = 'https://api.hnpwa.com/v0/item/@id.json';\r\nconst store = {   // 공유되는 자원, 객체\r\n  currentPage : 1,\r\n};\r\n\r\nfunction getData(url) {     // 중복되는 open, send, JSON.parse을 함수로 묶어 중복 최소화\r\n  ajax.open('GET', url, false);\r\n  ajax.send(); // 데이터 가져오기\r\n\r\n  return JSON.parse(ajax.response);\r\n}\r\n\r\nfunction newsFeed() {\r\n  const newsFeed = getData(NEWS_URL);\r\n  const newsList = [];\r\n  let template = `\r\n    <div class=\"container mx-auto p-4\">\r\n      <h1>Hacker News</h1>\r\n      <ul>\r\n        {{__news_feed__}}\r\n      </ul>\r\n      <div>\r\n        <a href=\"#/page/{{__prev_page__}}\">이전 페이지</a>\r\n        <a href=\"#/page/{{__next_page__}}\">다음 페이지</a>\r\n      </div>\r\n    </div>\r\n  `;\r\n \r\n  for(let i = (store.currentPage - 1) * 10; i < store.currentPage * 10; i++){    // 게시물 10개 가져오기\r\n  // DOM API를 최소화 사용, 문자열을 이용해서 마크업 구조 사용\r\n    newsList.push(`   \r\n      <li>\r\n        <a href=\"#/show/${newsFeed[i].id}\">\r\n          ${newsFeed[i].title} (${newsFeed[i].comments_count})\r\n        </a>\r\n      </li>\r\n    `);\r\n}\r\ntemplate = template.replace('{{__news_feed__}}', newsList.join(''));\r\ntemplate = template.replace('{{__prev_page__}}', store.currentPage > 1 ? store.currentPage - 1 : 1);\r\ntemplate = template.replace('{{__next_page__}}', store.currentPage + 1);\r\n\r\ncontainer.innerHTML = template;\r\n}\r\n\r\nfunction newsDetail() {\r\n  const id = location.hash.substring(7);\r\n  const newsContent = getData(CONTENT_URL.replace('@id', id))\r\n\r\n  container.innerHTML = `\r\n    <h1>${newsContent.title}</h1>\r\n\r\n    <div>\r\n      <a href=\"#/page/${store.currentPage}\">목록으로</a>\r\n    </div>\r\n  `;\r\n}\r\n\r\nfunction router() {\r\n  const routePath = location.hash;\r\n\r\n  if (routePath === '') {   // location.hash 값이 #이면 참으로 반환\r\n    newsFeed();\r\n  } else if (routePath.indexOf('#/page/') >= 0) {\r\n    store.currentPage = Number(routePath.substring(7));\r\n    newsFeed();\r\n  } else {\r\n    newsDetail();\r\n  }\r\n}\r\n\r\nwindow.addEventListener('hashchange', router);\r\n\r\nrouter();"},"lineCount":null}},"error":null,"hash":"3214a6c89b462b13c37ecab88962e39f","cacheData":{"env":{}}}